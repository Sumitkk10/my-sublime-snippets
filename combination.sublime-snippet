void nCr(){
	for(int i = 0; i < 20; i++){
		C[i][0] = C[i][i] = 1;
		for(int j = 1; j < i; j++){
			C[i][j] = C[i - 1][j] + C[i - 1][j - 1];
		}
	}
}
